<mvc:View height="100%"
	controllerName="com.axiomasoluciones.wisemobile.Modulo_WiseMobile.Main.controller.Menu"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Page id="idPage" showHeader="false" class="Degradado">
		<content>			
			<ObjectHeader title="{i18n>Title.3}" class="ZsapMOHTitle" />
			
			<FlexBox
				>
					<SlideTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout tile-hover-effect">
    					<tiles>
        					<GenericTile class="sapUiGenericTile" backgroundImage="wisemobile/Public/Img/1.jpg" frameType="TwoByOne" press="onPress"
							>
							<TileContent>
                			<Text text="{i18n>TileCollection.8.header}" class="sapMTileContentText"></Text>
            				</TileContent>
						<customData>
							<core:CustomData key="mod2">
							</core:CustomData>
						</customData>
        					</GenericTile>
    					</tiles>
					</SlideTile>
			</FlexBox>
		</content>
	</Page>
</mvc:View>